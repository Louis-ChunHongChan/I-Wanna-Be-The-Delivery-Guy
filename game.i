void updateGame() {
    frame++;


    if (!paused && !lost && !won) {
        if (frame % 10 == 0) {
            if (upward == 20) {
                ((screenblock *)0x6000000)[30].tilemap[((30) * (32) + (13))] = ((screenblock *)0x6000000)[30].tilemap[((21) * (32) + (13))];
                ((screenblock *)0x6000000)[30].tilemap[((30) * (32) + (14))] = ((screenblock *)0x6000000)[30].tilemap[((21) * (32) + (14))];
                ((screenblock *)0x6000000)[30].tilemap[((21) * (32) + (13))] = ((screenblock *)0x6000000)[30].tilemap[((2) * (32) + (1))];
                ((screenblock *)0x6000000)[30].tilemap[((21) * (32) + (14))] = ((screenblock *)0x6000000)[30].tilemap[((2) * (32) + (1))];
                upward = 29;
            }
            else {
                ((screenblock *)0x6000000)[30].tilemap[((upward) * (32) + (13))] = ((screenblock *)0x6000000)[30].tilemap[((upward+1) * (32) + (13))];
                ((screenblock *)0x6000000)[30].tilemap[((upward) * (32) + (14))] = ((screenblock *)0x6000000)[30].tilemap[((upward+1) * (32) + (14))];
                ((screenblock *)0x6000000)[30].tilemap[((upward+1) * (32) + (13))] = ((screenblock *)0x6000000)[30].tilemap[((2) * (32) + (1))];
                ((screenblock *)0x6000000)[30].tilemap[((upward+1) * (32) + (14))] = ((screenblock *)0x6000000)[30].tilemap[((2) * (32) + (1))];
                upward--;
            }
        }
    }


    if (vOff+1 < (768 - 160) && (guy.row - guyvOff) > 160 / 2) {
        guyvOff++;
        vOff++;
    }
    if (vOff-1 > 0 && guy.row - guyvOff <= 160 / 2) {

        guyvOff--;
        vOff--;
    }


    if (jumping) {
        if (frame - jumpFrame == 25) {
            jumping = 0;
            gravity = 10;
        }
    }


    if (!climbing && !jumping && !floating) {
        int kept = 0;
        while (collisionCheck(collisionMap, 256, guy.col, guy.row, guy.width, guy.height, 0, kept) && kept < gravity) {
            kept++;
        }
        guy.row += (kept-1);
    }


    if (down) {
        guy.aniState = DOWN;
        if (frame - downFrame == 30) {
            down = 0;
        }
    }


    if (jumpBoosted) {
        if (frame - bootsFrame == 120) {
            jumpBoosted = 0;
        }
    }


    if (!jumpBoosted) {
        if (frame - bootsFrame == 300) {
            boots.active = 1;
        }
    }

    if (floating) {
        if (collisionCheck(collisionMap, 256, guy.col, guy.row, guy.width, guy.height, 0, -guy.rdel) && guy.row > 0) {
            guy.row--;
        }
        if (frame - bubbleFrame == 130) {
            floating = 0;
            gravity = 10;
        }
    }

    if (!floating) {
        if (frame - bubbleFrame == 300) {
            for (int i = 0; i < 2; i++) {
                bubble[i].active = 1;
            }
        }

    }


    if (climbing && !collisionCheck(collisionMap, 256, guy.col, guy.row, guy.width, guy.height, 0, guy.rdel)) {

        climbing = 0;
    }

    updateGuy();
    updateDoor();
    updateKey();
    updateTele();
    updateBoots();
    updateHeart();
    updateShirt();
    updateBubble();
    updateCrazy();

}

void updateGuy() {

    if (life > 0 && collision(guy.col, guy.row, guy.width, guy.height*2, customer.col, customer.row, customer.width, customer.height)) {
        delivered = 1;
    }


    for (int i = 0; i < 9; i++) {
        if (crazy[i].active && collision(guy.col, guy.row, guy.width/2, guy.height/2, crazy[i].col, crazy[i].row, crazy[i].width/2, crazy[i].height/2)) {
            life--;
            down = 1;
            downFrame = frame;
            crazyFrame = frame;
            crazy[i].active = 0;
            playSoundB(sheidGuard_data, sheidGuard_length, 0);
        }
    }



    if (!down && !jumping && !floating) {
        if((~((*(volatile unsigned short *)0x04000130)) & ((1 << 6)))) {

            if (collisionLadderCheck(collisionMap, 256, guy.col + guy.width/2 - 1, guy.row, guy.width/4, guy.height, 0, -guy.rdel)) {
                guy.row -= guy.rdel;
                climbing = 1;


                if (vOff-1 >= 0 && (guy.row - guyvOff) <= 160/2) {

                    guyvOff -= guy.rdel;
                    vOff -= guy.rdel;
                }
            }
        }

        if((~((*(volatile unsigned short *)0x04000130)) & ((1 << 7)))) {
            if (collisionLadderCheck(collisionMap, 256, guy.col + guy.width/2 - 1, guy.row, guy.width/4, guy.height, 0, guy.rdel)) {
                guy.row += guy.rdel;
                climbing = 1;

                if ((vOff+1 < (768 - 160)) && ((guy.row - guyvOff) > 160 / 2)) {
                    guyvOff += guy.rdel;
                    vOff += guy.rdel;
                }
            }
        }
    }


    if (!climbing && !jumping && !down) {
        if((~((*(volatile unsigned short *)0x04000130)) & ((1 << 5)))) {
            if (collisionCheck(collisionMap, 256, guy.col, guy.row, guy.width, guy.height, -guy.cdel, 0) && guy.col > 0) {
                guy.col -= guy.cdel;

                if (hOff-1 >= 0 && (guy.col - guyhOff) <= 240/2) {
                    hOff -= guy.cdel;
                    guyhOff -= guy.cdel;
                }
            }
        }

        if((~((*(volatile unsigned short *)0x04000130)) & ((1 << 4)))) {
            if (collisionCheck(collisionMap, 256, guy.col, guy.row, guy.width, guy.height, guy.cdel, 0)
                    && guy.col + guy.width - 1 < 256) {
                guy.col += guy.cdel;

                if (hOff+1 < (256 - 240) && (guy.col - guyhOff) > 240 / 2) {
                    hOff += guy.cdel;
                    guyhOff += guy.cdel;
                }
            }
        }
    }

    animateGuy();

    if (vOff <= 512-160/2) {
        (*(volatile unsigned short *)0x4000008) = ((1) << 2) | ((28) << 8) | (2 << 14) | (0 << 7);
    }
    (*(volatile unsigned short *)0x04000012) = vOff;
}

