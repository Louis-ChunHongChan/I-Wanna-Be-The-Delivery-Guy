void animateGuy() {

    guy.prevAniState = guy.aniState;
    guy.aniState = IDLE;


    if(guy.aniCounter % 8 == 0) {
        guy.curFrame = (guy.curFrame + 1) % guy.numFrames;
    }


    if (!climbing && !down && !jumping) {
            if((~((*(volatile unsigned short *)0x04000130)) & ((1 << 5))) && !floating) {
                guy.aniState = LEFT;
            }
            if((~((*(volatile unsigned short *)0x04000130)) & ((1 << 4))) && !floating) {
                guy.aniState = RIGHT;
            }
            if (!down && !jumping && !floating) {

                if (guy.aniState == LEFT && (!(~(oldButtons) & ((1 << 1))) && (~buttons & ((1 << 1)))) && !collisionCheck(collisionMap, 256, guy.col, guy.row, guy.width, guy.height, 0, guy.rdel)) {
                    guy.aniState = LEFTJUMP;
                    jumping = 1;
                    jumpFrame = frame;
                    gravity = 0;

                    if (collisionCheck(collisionMap, 256, guy.col, guy.row, guy.width, guy.height, -15, -24)) {
                        if (jumpBoosted) {
                            guy.row -= 50;
                            guy.col -= 25;
                        }
                        else {
                            guy.row -= 24;
                            guy.col -= 15;
                        }
                    }

                }
                else if (guy.aniState == RIGHT && (!(~(oldButtons) & ((1 << 1))) && (~buttons & ((1 << 1)))) && !collisionCheck(collisionMap, 256, guy.col, guy.row, guy.width, guy.height, 0, guy.rdel)) {
                    guy.aniState = RIGHTJUMP;
                    jumping = 1;
                    jumpFrame = frame;
                    gravity = 0;
                    if (collisionCheck(collisionMap, 256, guy.col, guy.row, guy.width, guy.height, 15, -24)) {
                        if (jumpBoosted) {
                            guy.row -= 50;
                            guy.col += 25;
                        }
                        else {
                            guy.row -= 24;
                            guy.col += 15;
                        }
                    }
                }
            }

        }
        else if (!down && !jumping && !floating) {
            if((~((*(volatile unsigned short *)0x04000130)) & ((1 << 6))) || (~((*(volatile unsigned short *)0x04000130)) & ((1 << 7)))) {
                guy.aniState = CLIMB;
            }
        }

    if (guy.aniState == IDLE) {
        guy.curFrame = 0;
        guy.aniCounter = 0;
        guy.aniState = guy.prevAniState;
    }
    else {
        guy.aniCounter++;
    }

}

void animateCustomer() {

    customer.aniCounter++;

    if(customer.aniCounter % 10 == 0) {
        customer.curFrame = (customer.curFrame + 1) % customer.numFrames;
